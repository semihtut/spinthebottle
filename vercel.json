{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/pwa && npm install && npm run build",
  "outputDirectory": "apps/pwa/dist",
  "installCommand": "cd apps/pwa && npm install",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
